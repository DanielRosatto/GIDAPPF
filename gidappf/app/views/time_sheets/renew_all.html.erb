<p id="notice"><%= notice %></p>
<h1><%= t('body.gidappf_entity.time_sheet.action.renew.title') %></h1>
<%= form_with url: time_sheets_renew_all_path do |form| %>
  <div class="field">
    <%= form.label t('body.gidappf_entity.time_sheet.attributes.start_date')  %>
    <%= form.date_field :sd, as: :date, value: form.object.try(:strftime,"%m/%d/%Y"), class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label t('body.gidappf_entity.time_sheet.attributes.end_date')  %>
    <%= form.date_field :ed, as: :date, value: form.object.try(:strftime,"%m/%d/%Y"), class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn btn-default' %>
  </div>
<% end %>
